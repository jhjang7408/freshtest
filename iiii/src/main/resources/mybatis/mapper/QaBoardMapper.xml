<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multicampus.hhh.mapper.QaBoardMapper">

    <select id="findAll" resultType="com.multicampus.hhh.domain.QaBoard">
        SELECT * FROM qa_board;
    </select>


    <select id="findById" parameterType="int" resultType="com.multicampus.hhh.domain.QaBoard">
        select * from qa_board where qa_id = #{qaid};
    </select>




</mapper>